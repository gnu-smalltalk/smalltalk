/* String similarityTo: */

primitive VMpr_String_similarityTo [succeed,fail]
{
  int result, l1, l2;
  gst_uchar *s1, *s2;
  OOP oop1, oop2;
  _gst_primitives_executed++;

  oop2 = POP_OOP ();
  oop1 = STACKTOP ();

  if (IS_INT(oop2)
      || OOP_FIXED_FIELDS (oop2)
      || (OOP_INSTANCE_SPEC (oop2) & ISP_INDEXEDVARS) != GST_ISP_CHARACTER)
    PRIM_FAILED;

