  if (IS_INT(oop1)
      || OOP_FIXED_FIELDS (oop1)
      || (OOP_INSTANCE_SPEC (oop1) & ISP_INDEXEDVARS) != GST_ISP_CHARACTER)
    PRIM_FAILED;
